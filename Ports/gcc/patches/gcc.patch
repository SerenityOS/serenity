+
+#undef SUBTARGET_EXTRA_ASM_SPEC
+#define SUBTARGET_EXTRA_ASM_SPEC	\
+  "%{" FPIE_OR_FPIC_SPEC ":-k}"
+
+/* Default to full VFP if -mfloat-abi=hard is specified.  */
