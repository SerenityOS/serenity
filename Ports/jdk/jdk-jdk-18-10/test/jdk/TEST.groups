    :jdk_instrument \
    :jdk_jmx \
    :jdk_jdi \
    :jdk_jfr \
    :svc_tools

jdk_foreign = \
    java/foreign \
    -java/foreign/TestMatrix.java

jdk_vector = \
    jdk/incubator/vector

