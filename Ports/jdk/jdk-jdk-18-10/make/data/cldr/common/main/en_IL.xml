	<dates>
		<calendars>
			<calendar type="generic">
				<dateTimeFormats>
					<availableFormats>
						<dateFormatItem id="EHm">E H:mm</dateFormatItem>
						<dateFormatItem id="EHms">E H:mm:ss</dateFormatItem>
						<dateFormatItem id="H">H</dateFormatItem>
