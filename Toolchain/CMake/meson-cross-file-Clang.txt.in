[binaries]
c = '@SERENITY_ARCH@-pc-serenity-clang'
cpp = '@SERENITY_ARCH@-pc-serenity-clang++'
ar = 'llvm-ar'
ranlib = 'llvm-ranlib'
pkgconfig = 'pkg-config'

[properties]
sys_root = '@SERENITY_BUILD_DIR@/Root'
needs_exe_wrapper = true

[host_machine]
system = 'serenity'
cpu_family = '@SERENITY_ARCH@'
cpu = '@SERENITY_ARCH@'
endian = 'little'
